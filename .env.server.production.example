# Production server environment variables
# Copy this to .env.server.production and fill in your values

DATABASE_URL=postgresql://user:password@localhost/dbname?host=/cloudsql/project-id:region:instance-name
JWT_SECRET=your-production-jwt-secret-min-32-chars

ADMIN_EMAIL=your-email@example.com

MAILGUN_API_URL=https://api.mailgun.net
MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_DOMAIN=your-domain.com

SMS_API_KEY=your-sms-api-key

WASP_WEB_CLIENT_URL=https://your-domain.com
WASP_SERVER_URL=https://your-server-url.run.app
